# Demo Environment (Client Presentation)
VITE_APP_ENV=demo

# Domain Configuration
VITE_PUBLIC_DOMAIN=demo.pickpoint.my.id
VITE_ADMIN_DOMAIN=demo.pickpoint.my.id

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_DEBUG_MODE=false

# Storage
VITE_STORAGE_PREFIX=pickpoint_demo_

# Security
VITE_SESSION_TIMEOUT=7200000
VITE_MAX_LOGIN_ATTEMPTS=10
